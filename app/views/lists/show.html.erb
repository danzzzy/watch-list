<div class="container">
<p><%= @list.name %></p>
<% @list.bookmarks.each do |bookmark| %>
  <p><%= bookmark.movie.title %></p>
  <p><%= image_tag(bookmark.movie.poster_url) %></p>
<%= link_to "delete", bookmark_path(bookmark), method: :delete, data: { confirm: "Are you sure you would like to delete #{bookmark.movie.title}?"} %>
<% end %>
<%= link_to "Add bookmark", new_list_bookmark_path(@list) %>
</div>
